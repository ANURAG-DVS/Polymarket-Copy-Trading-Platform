{
    "dashboard": {
        "id": null,
        "uid": "polymarket-trader-dashboard",
        "title": "Polymarket Copy Trading - Trader Data Monitoring",
        "tags": [
            "polymarket",
            "traders",
            "performance"
        ],
        "timezone": "browser",
        "schemaVersion": 27,
        "version": 0,
        "refresh": "30s",
        "panels": [
            {
                "id": 1,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                },
                "type": "graph",
                "title": "Trader Fetch Success Rate",
                "targets": [
                    {
                        "expr": "rate(trader_fetch_success_total[5m])",
                        "legendFormat": "Success Rate",
                        "refId": "A"
                    },
                    {
                        "expr": "rate(trader_fetch_error_total[5m])",
                        "legendFormat": "Error Rate",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "percentunit",
                        "label": "Success Rate"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "legend": {
                    "show": true
                }
            },
            {
                "id": 2,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                },
                "type": "graph",
                "title": "Leaderboard Query Performance",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(leaderboard_query_duration_seconds_bucket[5m]))",
                        "legendFormat": "p95",
                        "refId": "A"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(leaderboard_query_duration_seconds_bucket[5m]))",
                        "legendFormat": "p50",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s",
                        "label": "Duration"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                }
            },
            {
                "id": 3,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 0,
                    "y": 8
                },
                "type": "stat",
                "title": "Cache Hit Ratio",
                "targets": [
                    {
                        "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m]))",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percentunit",
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": null,
                                    "color": "red"
                                },
                                {
                                    "value": 0.7,
                                    "color": "yellow"
                                },
                                {
                                    "value": 0.85,
                                    "color": "green"
                                }
                            ]
                        }
                    }
                }
            },
            {
                "id": 4,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 8,
                    "y": 8
                },
                "type": "stat",
                "title": "Total Traders",
                "targets": [
                    {
                        "expr": "total_traders_count",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short"
                    }
                }
            },
            {
                "id": 5,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 16,
                    "y": 8
                },
                "type": "stat",
                "title": "Data Freshness (minutes)",
                "targets": [
                    {
                        "expr": "time() - trader_last_update_timestamp",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "dtdurationms",
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": null,
                                    "color": "green"
                                },
                                {
                                    "value": 600,
                                    "color": "yellow"
                                },
                                {
                                    "value": 1800,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                }
            },
            {
                "id": 6,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                },
                "type": "graph",
                "title": "Celery Task Execution Time",
                "targets": [
                    {
                        "expr": "rate(celery_task_duration_seconds_sum{task=\"fetch_top_traders_task\"}[5m])",
                        "legendFormat": "Fetch Traders",
                        "refId": "A"
                    },
                    {
                        "expr": "rate(celery_task_duration_seconds_sum{task=\"calculate_leaderboard_task\"}[5m])",
                        "legendFormat": "Calculate Leaderboard",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s",
                        "label": "Duration"
                    }
                ]
            },
            {
                "id": 7,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 16
                },
                "type": "graph",
                "title": "API Request Rate",
                "targets": [
                    {
                        "expr": "rate(http_requests_total{endpoint=~\"/api/v1/traders.*\"}[5m])",
                        "legendFormat": "{{endpoint}}",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "reqps",
                        "label": "Requests/sec"
                    }
                ]
            },
            {
                "id": 8,
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 24
                },
                "type": "table",
                "title": "Recent Task Failures",
                "targets": [
                    {
                        "expr": "celery_task_failures_total",
                        "refId": "A",
                        "format": "table"
                    }
                ],
                "transformations": [
                    {
                        "id": "organize",
                        "options": {
                            "excludeByName": {},
                            "indexByName": {},
                            "renameByName": {
                                "task": "Task Name",
                                "Value": "Failure Count"
                            }
                        }
                    }
                ]
            }
        ],
        "time": {
            "from": "now-6h",
            "to": "now"
        },
        "timepicker": {
            "refresh_intervals": [
                "10s",
                "30s",
                "1m",
                "5m",
                "15m",
                "30m"
            ]
        },
        "annotations": {
            "list": [
                {
                    "datasource": "Prometheus",
                    "enable": true,
                    "expr": "changes(app_version[5m]) > 0",
                    "iconColor": "blue",
                    "name": "Deployments",
                    "tagKeys": "version",
                    "titleFormat": "Deployment"
                }
            ]
        }
    }
}